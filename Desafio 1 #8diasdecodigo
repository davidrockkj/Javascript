var number = prompt("NUMBER: ");
var workedHours = prompt("WORKED HOURS: ");
var salary = prompt("SALARY (USD): "); 

function finalSalary (salary, workedHours){
  var salaryHour = (salary*workedHours);
  return 'U$ ' + salaryHour.toFixed(2);
}

console.log('NUMBER: ' + number);
console.log('SALARY: ' + finalSalary(salary, workedHours));
